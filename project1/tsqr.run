#!/bin/bash -l
#SBATCH --nodes=32
#SBATCH --ntasks-per-node=2
#SBATCH --cpus-per-task=1
#SBATCH --time=01:30:00
#SBATCH --qos=parallel
#SBATCH --account=math-505
#SBATCH --output=/dev/null

run_number=$1
col_number=$2


module load gcc openmpi python py-mpi4py
srun --output="./runs/tsqr/tsqr_run_${run_number}_8192_${col_number}_64.txt" python tsqr_with_sparse.py ${col_number}
